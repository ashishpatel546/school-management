{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "${process.env.NEXT_PUBLIC_API_URL}/:path*"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "https://school-management-app-tunnel-ah2j29o2.devinapps.com"
  }
}
