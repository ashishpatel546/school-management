{
  "version": 2,
  "builds": [
    {
      "src": "dist/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/main.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "DATABASE_TYPE": "sqlite",
    "DATABASE_NAME": ":memory:",
    "PORT": "8000"
  }
}
